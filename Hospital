 static void Main(string[] args)
        {
            int amountVisitors;
            int timePatientExamination = 10;
            int durationOfAllAdmission;
            int hoursToEndOfWork;
            int minutesToEndOfWork;

            Console.Write("Укажите количество пациентов к вам на прием?");
            amountVisitors = Convert.ToInt32(Console.ReadLine());
            durationOfAllAdmission = amountVisitors * timePatientExamination;
            hoursToEndOfWork = durationOfAllAdmission / 60;
            minutesToEndOfWork = durationOfAllAdmission % 60;
            Console.WriteLine($"Ваше рабочее время закончиться через : {hoursToEndOfWork} Часа {minutesToEndOfWork} Минут");
        }
