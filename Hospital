 static void Main(string[] args)
        {
            int amountVisitors;
            int timePatientExamination = 10;
            int workTime;
            int hours;
            int minutes;

            Console.Write("Укажите количество пациентов к вам на прием?");
            amountVisitors = Convert.ToInt32(Console.ReadLine());
            workTime = amountVisitors * timePatientExamination;
            hours = workTime / 60;
            minutes = workTime % 60;
            Console.WriteLine($"Ваше рабочее время закончиться через : {hours} Часа {minutes} Минут");           
        }
