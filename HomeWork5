 static void Main(string[] args)
        {
            int amountVisitors;
            int timePatientExamination = 10;
            int durationOfAllAdmission;
            int hoursToEndOfJob;
            int minutesToEndOfJob;

            Console.Write("Укажите количество пациентов к вам на прием?");
            amountVisitors = Convert.ToInt32(Console.ReadLine());
            durationOfAllAdmission = amountVisitors * timePatientExamination;
            hoursToEndOfJob = durationOfAllAdmission / 60;
            minutesToEndOfJob = durationOfAllAdmission % 60;
            Console.WriteLine($"Ваше рабочее время закончиться через : {hoursToEndOfJob} Часа {minutesToEndOfJob} Минут");
        }
